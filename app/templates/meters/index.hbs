<div>
  {{#mdc-list}}
    {{#each model as |meter|}}
      {{#mdc-list-item-link-to "meters.chart" meter.id currentYear currentMonth currentDay currentHour trailingIcon="arrow_forward"}}
        {{#mdc-list-item-primary-text}}Meter Id: {{meter.id}}{{/mdc-list-item-primary-text}}
        {{#mdc-list-item-secondary-text}} {{meter.serviceType}}{{/mdc-list-item-secondary-text}}
        {{mdc-list-item-meta-icon "arrow_forward"}}
      {{/mdc-list-item-link-to}}
    {{/each}}
  {{/mdc-list}}
</div>